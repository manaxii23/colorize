{
    "manifest_version": 3,
    "name": "colorize",
    "version": "1.1.2",
    "description": "Pick your color the easy way!",
    "icons": {
      "16": "logo128.png",
      "48": "logo128.png",
      "128": "logo128.png"
    },
    "action": {
      "default_icon": "logo128.png",
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }